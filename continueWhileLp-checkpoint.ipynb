    "value = 123456\n",
    "while value > 1:\n",
    "    div = int(input (\"Enter a value to be divided: \"))\n",
    "    if div == 0:\n",
    "        print (\"You cannot divide by 0!\")\n",
    "        continue\n",
    "    value /= div\n",
    "    print (\"The value is now \", value)\n",
    "print (\"Done with the loop\")"
